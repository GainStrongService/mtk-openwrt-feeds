+++ b/src/drivers/driver.h
@@ -2804,6 +2804,11 @@ struct wpa_mlo_signal_info {
 	struct wpa_signal_info links[MAX_NUM_MLD_LINKS];
 };
 
+struct wpa_neg_ttlm_info {
