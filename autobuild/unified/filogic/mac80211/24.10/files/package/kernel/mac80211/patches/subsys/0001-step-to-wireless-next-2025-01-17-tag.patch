-	}
-
-	chip = &plfxlc_hw_mac(hw)->chip;
-	usb = &chip->usb;
-	usb->ez_usb = intf;
-	tx = &usb->tx;
-
-	r = plfxlc_upload_mac_and_serial(intf, hw_address, serial_number);
-	if (r) {
-		dev_err(&intf->dev, "MAC and Serial upload failed (%d)\n", r);
