-		hwsim_tx_virtio(data, skb);
-	else
-		hwsim_unicast_netgroup(data, skb, _portid);
-
-	data->pmsr_request = request;
-	data->pmsr_request_wdev = ieee80211_vif_to_wdev(vif);
-
-out_free:
-	if (err && skb)
-		nlmsg_free(skb);
-
-	mutex_unlock(&data->mutex);
-	return err;
