-/* Called with the rtnl lock held. */
-static int virt_wifi_dump_station(struct wiphy *wiphy, struct net_device *dev,
-				  int idx, u8 *mac, struct station_info *sinfo)
-{
-	struct virt_wifi_netdev_priv *priv = netdev_priv(dev);
-
