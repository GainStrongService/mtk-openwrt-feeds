-	int err;
-
-	wiphy = wiphy_new(&virt_wifi_cfg80211_ops, sizeof(*priv));
-
-	if (!wiphy)
-		return NULL;
-
-	wiphy->max_scan_ssids = 4;
-	wiphy->max_scan_ie_len = 1000;
-	wiphy->signal_type = CFG80211_SIGNAL_TYPE_MBM;
-
-	wiphy->bands[NL80211_BAND_2GHZ] = &band_2ghz;
