-		goto err;
-	}
-
-	priv->rxhw = synth & PDR_SYNTH_FRONTEND_MASK;
-
-	err = p54_generate_channel_lists(dev);
-	if (err)
-		goto err;
-
-	if (priv->rxhw == PDR_SYNTH_FRONTEND_XBOW)
-		p54_init_xbow_synth(priv);
-	if (!(synth & PDR_SYNTH_24_GHZ_DISABLED))
