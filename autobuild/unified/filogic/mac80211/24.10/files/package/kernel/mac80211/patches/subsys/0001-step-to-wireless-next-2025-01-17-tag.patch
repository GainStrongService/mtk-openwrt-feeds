-static int p54_rssi_to_dbm(struct p54_common *priv, int rssi)
-{
-	if (priv->rxhw != 5) {
-		return ((rssi * priv->cur_rssi->mul) / 64 +
-			 priv->cur_rssi->add) / 4;
-	} else {
-		/*
-		 * TODO: find the correct formula
-		 */
-		return rssi / 2 - 110;
-	}
-}
