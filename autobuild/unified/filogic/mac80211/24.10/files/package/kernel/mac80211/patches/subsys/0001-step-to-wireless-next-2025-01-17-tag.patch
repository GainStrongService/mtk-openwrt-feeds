-/* Called with rtnl lock held. */
-static int virt_wifi_net_device_open(struct net_device *dev)
-{
-	struct virt_wifi_netdev_priv *priv = netdev_priv(dev);
-
-	priv->is_up = true;
-	return 0;
-}
