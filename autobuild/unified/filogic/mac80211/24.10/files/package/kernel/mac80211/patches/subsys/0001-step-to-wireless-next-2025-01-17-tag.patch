-static struct rtnl_link_ops virt_wifi_link_ops = {
-	.kind		= "virt_wifi",
-	.setup		= virt_wifi_setup,
-	.newlink	= virt_wifi_newlink,
-	.dellink	= virt_wifi_dellink,
