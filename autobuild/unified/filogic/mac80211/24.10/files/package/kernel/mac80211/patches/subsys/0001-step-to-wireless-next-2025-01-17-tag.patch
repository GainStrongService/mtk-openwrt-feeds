-		}
-		txi->flags |= IEEE80211_TX_STAT_ACK;
-	}
-
-	if (hwsim_flags & HWSIM_TX_CTL_NO_ACK)
-		txi->flags |= IEEE80211_TX_STAT_NOACK_TRANSMITTED;
