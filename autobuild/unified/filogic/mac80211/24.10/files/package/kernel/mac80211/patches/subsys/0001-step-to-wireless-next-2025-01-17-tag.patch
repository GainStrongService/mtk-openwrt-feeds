-	case -ESHUTDOWN:
-	case -EINVAL:
-	case -ENODEV:
-	case -ENOENT:
-	case -ECONNRESET:
-	case -EPIPE:
-		dev_dbg(plfxlc_urb_dev(urb), "urb %p error %d\n", urb, urb->status);
-		return;
-	default:
