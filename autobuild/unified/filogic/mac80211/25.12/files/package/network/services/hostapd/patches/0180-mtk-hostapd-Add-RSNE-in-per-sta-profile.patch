 src/ap/ieee802_11.h     |  4 ++-
 src/ap/ieee802_11_eht.c |  2 +-
 3 files changed, 59 insertions(+), 7 deletions(-)

diff --git a/src/ap/ieee802_11.c b/src/ap/ieee802_11.c
index d31011c65..e8dc09433 100644
