+++ b/src/ap/ieee802_11.c
@@ -4938,14 +4938,51 @@ static int check_assoc_ies(struct hostapd_data *hapd, struct sta_info *sta,
 }
 
 
+static bool is_rsne_present_in_assoc_resp(struct wpa_state_machine *sm,
+					  struct hostapd_data *hapd,
+					  u8 **pmkid)
+{
+	switch (sm->auth_alg) {
+	case WLAN_AUTH_FT:
+#ifdef CONFIG_IEEE80211R_AP
+		*pmkid = sm->pmk_r1_name;
+#endif
+		return true;
+	case WLAN_AUTH_FILS_SK:
+	case WLAN_AUTH_FILS_SK_PFS:
+	case WLAN_AUTH_FILS_PK:
+#ifdef CONFIG_IEEE80211R_AP
+		if (sm->wpa_key_mgmt & (WPA_KEY_MGMT_FT_FILS_SHA256 |
+					WPA_KEY_MGMT_FT_FILS_SHA384))
+			*pmkid = sm->pmk_r1_name;
+		else
+#endif
+			*pmkid = NULL;
+
+		return true;
+	default:
+		break;
+	}
+
+	if (hapd->conf->wpa_key_mgmt & WPA_KEY_MGMT_OWE) {
+		*pmkid = sm->pmksa ? sm->pmksa->pmkid: NULL;
+		return true;
+	}
+
+	return false;
+}
+
