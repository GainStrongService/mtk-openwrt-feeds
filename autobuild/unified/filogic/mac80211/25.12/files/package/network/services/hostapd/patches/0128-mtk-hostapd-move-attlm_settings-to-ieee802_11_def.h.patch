- * the bits 10 to 25 of the TSF
- * @duration_tu: duration in ms that the A-TTLM lasts
- * @start_time: the relative time that this A-TTLM is entablished
- */
-struct attlm_settings {
-	bool valid;
-	u8 direction;
-	u16 disabled_links;
-	u8 link_mapping_size;
-	u16 switch_time;
-	u16 switch_time_tsf_tu;
