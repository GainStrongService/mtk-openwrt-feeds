+		val = an8855_reg_read(gsw, QP_DIG_MODE_CTRL_0);
+		val &= ~BIT(0);
+		val &= ~BITS(4, 5);
+		an8855_reg_write(gsw, QP_DIG_MODE_CTRL_0, val);
