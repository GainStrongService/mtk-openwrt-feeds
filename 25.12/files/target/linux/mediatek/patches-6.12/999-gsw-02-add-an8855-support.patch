+	/* Frequency memter */
+	val = an8855_reg_read(gsw, RX_CTRL_5);
+	val &= ~(0xfffff << 10);
+	val |= (0x28 << 10);
+	an8855_reg_write(gsw, RX_CTRL_5, val);
+
+	val = an8855_reg_read(gsw, RX_CTRL_6);
+	val &= ~(0xfffff << 0);
+	val |= (0x64 << 0);
+	an8855_reg_write(gsw, RX_CTRL_6, val);
