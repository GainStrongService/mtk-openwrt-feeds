+		val = an8855_reg_read(gsw, RG_RATE_ADAPT_CTRL_0);
+		val |= BIT(0);
+		val |= BIT(4);
+		val |= BITS(26, 27);
+		an8855_reg_write(gsw, RG_RATE_ADAPT_CTRL_0, val);
