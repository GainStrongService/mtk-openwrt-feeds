+	val &= ~(0xf << 16);
+	val |= (0x4 << 16);
+	an8855_reg_write(gsw, PLL_CTRL_2, val);
+
+	/* PLL - PFD */
+	val = an8855_reg_read(gsw, PLL_CTRL_2);
