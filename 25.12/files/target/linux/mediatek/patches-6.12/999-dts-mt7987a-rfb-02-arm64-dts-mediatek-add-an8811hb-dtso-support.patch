From 15c0a511ac8500cfc9ce4a94110228456fa35d85 Mon Sep 17 00:00:00 2001
From: Mason Chang <mason-cw.chang@mediatek.com>
Date: Thu, 18 Dec 2025 18:00:53 +0800
Subject: [PATCH] arm64: dts: mediatek: add an8811hb dtso support

---
 .../mediatek/mt7987-netsys-eth0-an8811hb.dtso | 42 +++++++++++++++++++
 1 file changed, 42 insertions(+)
 create mode 100644 arch/arm64/boot/dts/mediatek/mt7987-netsys-eth0-an8811hb.dtso

diff --git a/arch/arm64/boot/dts/mediatek/mt7987-netsys-eth0-an8811hb.dtso b/arch/arm64/boot/dts/mediatek/mt7987-netsys-eth0-an8811hb.dtso
new file mode 100644
index 0000000..b7e3cb5
--- /dev/null
+++ b/arch/arm64/boot/dts/mediatek/mt7987-netsys-eth0-an8811hb.dtso
@@ -0,0 +1,42 @@
+// SPDX-License-Identifier: (GPL-2.0 OR MIT)
+
+/dts-v1/;
+/plugin/;
+
+/ {
+	fragment@0 {
+		target = <&gmac0>;
+		__overlay__ {
+			phy-mode = "2500base-x";
+			phy-handle = <&phy12>;
+			status = "okay";
+		};
+	};
+
+	fragment@1 {
+		target-path = "/soc_netsys/ethernet@15100000/mdio-bus";
+		__overlay__ {
+			phy12: phy@12 {
+				compatible = "ethernet-phy-idc0ff.04a0";
+				reg = <12>;
+				reset-gpios = <&pio 48 1>;
+				reset-assert-us = <10000>;
+				reset-deassert-us = <10000>;
+				phy-mode = "2500base-x";
+				full-duplex;
+				pause;
+				airoha,phy-handle;
+				eee-broken-100tx;
+				eee-broken-1000t;
+			};
+		};
+	};
+
+	fragment@2 {
+		target-path = "/soc_netsys/hnat@15000000";
+		__overlay__ {
+			mtketh-lan = "eth0";
+		};
+	};
+};
+
-- 
2.45.2

